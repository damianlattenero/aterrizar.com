<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class 
name="ar.edu.unq.epers.aterrizar.model.Tramo" table="tramo">
<id column="id" name="id" type="string"> </id>
	<property column="origen" generated="never" lazy="false" length="45" name="destino" type="string"/>
	<property column="destino" generated="never" lazy="false" length="45" name="destino" type="string"/>
	<property column="horaLlegada" generated="never" lazy="false" length="45" name="horaLlegada" type="date"/>
	<property column="horaSalida" generated="never" lazy="false" length="45" name="horaSalida" type="date"/>
	<property column="precioBase" generated="never" lazy="false" length="45" name="precioBase" type="float"/>
	<bag fetch="select" inverse="true" lazy="true" name="asientos" table="asientos">
	<key><column name="id" not-null="true"/></key><one-to-many class="ar.edu.unq.epers.aterrizar.model.Asiento"/>
	</bag></class>
</hibernate-mapping>
