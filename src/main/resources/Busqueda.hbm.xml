<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class
            name="ar.edu.unq.epers.aterrizar.model.Busqueda" table="busqueda">
        <id column="id" name="id" type="integer">
            <generator class="identity"/>
        </id>


        <many-to-one cascade="all" column="criterio" name="criterio"
                     class="ar.edu.unq.epers.aterrizar.model.Criterio"/>
        <many-to-one cascade="all" column="orden" name="orden"
                     class="ar.edu.unq.epers.aterrizar.model.Orden" lazy="false" fetch="select"/>

    </class>
</hibernate-mapping>
